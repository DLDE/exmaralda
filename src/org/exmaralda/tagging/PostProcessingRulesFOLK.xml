    </rule>
    <rule>
        <!-- PAV wird zu ADV -->
        <match field="pos">PAV</match>
        <set field="pos">ADV</set>
    </rule>
    <rule>
        <!-- 'gell' ist immer SEQU -->
        <match field="n">gell</match>
        <set field="pos">SEQU</set>
    </rule>    
